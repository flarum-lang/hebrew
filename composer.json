{
    "name": "flabrew/flarum-hebrew",
    "type": "flarum-extension",
    "description": "תרגום Flarum רשמי לעברית.",
    "homepage": "https://github.com/icecore2/Flarum-hebrew",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0"
    },
	
    "extra": {
        "flarum-extension": {
            "title": "השפה העברית",
            "icon": {
                "image": "icon.png",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "he",
            "title": "עברית"
		}
    },
	"extiverse": {
      "discuss": "https://discuss.flarum.org/d/6473"
    }
}
